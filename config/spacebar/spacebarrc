spacebar -m config spacing_right               15
spacebar -m config title                       on
spacebar -m config spaces                      on
spacebar -m config clock                       on
spacebar -m config power                       off
spacebar -m config text_font                   "Liga SFMono Nerd Font:Regular:13.0"
spacebar -m config icon_font                   "Liga SFMono Nerd Font:Regular:18.0"
spacebar -m config background_color            0xFF161616
spacebar -m config foreground_color            0xFF3DDBD9
spacebar -m config dnd_icon_color              0xff78c4d4
spacebar -m config clock_icon_color            0xFF161616
