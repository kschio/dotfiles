        "syntax": {
          "constant": {
            "color": "#999999",
            "font_style": null,
            "font_weight": null
          },
          "string": {
            "color": "#999999",
            "font_style": null,
            "font_weight": null
          },
          "comment": {
            "color": "#666666",
            "font_style": null,
            "font_weight": null
          },
          "keyword": {
            "color": "#999999",
            "font_style": null,
            "font_weight": null
          },
          "parameter": {
            "color": "#999999",
            "font_style": null,
            "font_weight": null
          },
          "punctuation": {
            "color": "#999999",
            "font_style": null,
            "font_weight": null
          },
          "property": {
            "color": "#eeeeee",
            "font_style": null,
            "font_weight": null
          },
          "function": {
            "color": "#eeeeee",
            "font_style": null,
            "font_weight": 600
          },
          "number": {
            "color": "#eeeeee",
            "font_style": null,
            "font_weight": null
          }
        }
